<UserControl x:Class="SControlBase.Demo.TransitionsOne"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:wpf="clr-namespace:SControlBase"
             xmlns:local="clr-namespace:SControlBase.Demo"
             
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ResourceDictionary >
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SControlBase;component/Themes/ButtonStyle.xaml"></ResourceDictionary>
            <ResourceDictionary Source="pack://application:,,,/SControlBase;component/Themes/ScheduleSliderStyle.xaml"></ResourceDictionary>
        </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Background="White">
        <wpf:RenderButton   Command="{x:Static wpf:Transitioner.MoveNextCommand}"
                HorizontalAlignment="Center" VerticalAlignment="Center" Background="#eb4b4b" Content="demo" Height="30" Width="150">
           
        </wpf:RenderButton>
    </Grid>
</UserControl>
